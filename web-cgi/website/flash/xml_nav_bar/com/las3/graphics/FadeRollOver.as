package com.las3.graphics { 	import flash.display.Sprite;	import flash.events.*;	    public class FadeRollOver extends Sprite {		        private var _alphaDest:Number; 		public function FadeRollOver() {			_alphaDest =  .65;			alpha = _alphaDest;			addEventListener(MouseEvent.ROLL_OVER, onOver, false, 0, true);			addEventListener(MouseEvent.ROLL_OUT, onOut, false, 0, true);		   		}				private function onOver(evt:MouseEvent):void {			_alphaDest = 1;			addEventListener(Event.ENTER_FRAME, onLoop, false, 0, true); 		}				private function onOut(evt:MouseEvent):void {			_alphaDest = .65;			 		}				private function onLoop(evt:Event):void {			if (Math.abs(alpha - _alphaDest) > .01) {				alpha += (_alphaDest - alpha)/4;			} else {				removeEventListener(Event.ENTER_FRAME, onLoop); 			}		}	} }