package {		import flash.display.*;	import flash.events.*;		public class DeleteXML extends Sprite {				private var book:XML;				public function DeleteXML() {			createBasicStructure();			removeTextNode();			removeElementNode();			removeAttribute();		}		private function createBasicStructure() {			book =	<book>						<publisher name="O'Reilly"/>						<title>Learning ActionScript 3.0</title>						<subject>ActionScript</subject>					</book>			trace(book);		}				private function removeTextNode() {			delete book.subject.text()[0];			trace(book);		}				private function removeElementNode() {			delete book.subject;			trace(book);		}				private function removeAttribute() {			delete book.publisher.@name;			trace(book);		}	}}